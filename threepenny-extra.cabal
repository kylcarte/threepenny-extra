name: threepenny-extra
version: 0.1.0.0
cabal-version: >=1.10
build-type: Simple
license: BSD3
license-file: LICENSE
maintainer: kylcarte@gmail.com
author: Kyle Carter

library
    exposed-modules:
        Test
        Graphics.UI.Threepenny.Widgets.DataTable
        Graphics.UI.Threepenny.Attributes.Extra
        Graphics.UI.Threepenny.Elements.ID
        Graphics.UI.Threepenny.Widgets.Plugins
