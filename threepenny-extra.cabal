        Graphics.UI.Threepenny.Table
    build-depends:
        base >=4.8 && <4.9,
        aeson -any,
        bytestring -any,
        containers -any,
        contravariant -any,
        distributive -any,
        lens -any,
        lens-action -any,
        text -any,
        threepenny-gui >=0.6.0.4,
        uuid -any
    default-language: Haskell2010
    hs-source-dirs: src

